<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<deployment>
    <cluster sitesperhost="6"/>
    <partition-detection/>
    <heartbeat/>
    <ssl/>
    <httpd enabled="true">
        <jsonapi enabled="true"/>
    </httpd>
    <snapshot frequency="15s" enabled="true"/>
    <commandlog enabled="false">
        <frequency/>
    </commandlog>
    <systemsettings>
        <temptables maxsize="200"/>
        <snapshot/>
        <elastic/>
        <query/>
        <procedure/>
        <resourcemonitor>
            <memorylimit/>
        </resourcemonitor>
        <flushinterval>
            <dr/>
            <export/>
        </flushinterval>
    </systemsettings>
    <security enabled="false"/>
</deployment>
